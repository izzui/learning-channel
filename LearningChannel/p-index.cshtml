	<meta name="author" content="">

	<title>Demo - Learning Channels</title>
	
	<!-- icons -->
	<link rel="stylesheet" type="text/css" href="css/font-awesome.css">
		
	<!-- bootstrap core CSS -->	
	<link rel="stylesheet" type="text/css" href="css/bootstrap.css">
	
	<!-- demo-izzui -->
	<link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family=Share:400,400italic,700,700italic"><!-- outros textos -->
	<link rel="stylesheet" type="text/css" href="css/demo-izzui.css">

	<!-- client's theme -->
	<link rel='stylesheet' type='text/css' href='//fonts.googleapis.com/css?family=Enriqueta:400,700&subset=latin,latin-ext'> <!-- tipografia para a marca e títulos -->
